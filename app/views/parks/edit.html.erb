<h1> Edit Park </h1>

<%= form_for (@park) do |f| %>
  <div class="field">
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :location %><br>
  <%= f.text_field :location %>
</div>
<div class="field">
    <%= f.label :image %><br>
    <%= f.file_field :image %>
</div>
<%= f.hidden_field :user_id, :value => current_user.id%>
      <%= f.submit "Submit", class:"submit" %>
<% end %>



